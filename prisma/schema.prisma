// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model merchant {
  id        Int      @unique
  createdAt DateTime @default(now())
  name      String?
  email     String   @unique
  category  String
  address   String
}
